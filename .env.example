# Job Dashboard Configuration

# Service Selection - Choose between mock data and real API
REACT_APP_USE_MOCK_DATA=true

# Real API Configuration (used when REACT_APP_USE_MOCK_DATA=false)
REACT_APP_API_BASE_URL=https://localhost:5001/api
REACT_APP_SIGNALR_HUB_URL=https://localhost:5001/jobHub

# Optional: Error Handling Configuration
REACT_APP_MAX_RETRIES=3
REACT_APP_RETRY_DELAY=1000

# Optional: Debug Mode
REACT_APP_DEBUG=false
